        </activity>
        <activity android:name=".Activities.authorActivities.AuthorizationOrRegistrationActivity" />

        <activity android:name=".Activities.authorActivities.RegistrationActivity">
        </activity>

        <activity android:name=".Activities.Menu.NewMainClass">

